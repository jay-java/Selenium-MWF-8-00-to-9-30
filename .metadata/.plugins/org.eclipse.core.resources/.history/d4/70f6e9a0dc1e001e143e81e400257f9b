package basic;
//1.Arithmetic Operators->+,-,*,/,%
public class Operators {
	public static void main(String[] args) {
		int i=10,j=20,k;
		System.out.println(i+j);
		System.out.println(i-j);
		System.out.println(i*j);
		System.out.println(i/j);
		System.out.println(i%j);
		k = i+j;
		System.out.println("k = "+k);
	}
}
