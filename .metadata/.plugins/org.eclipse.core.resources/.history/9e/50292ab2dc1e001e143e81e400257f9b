package basic;
//1.Arithmetic Operators->+,-,*,/,%
public class Operators {
	public static void main(String[] args) {
		int i=5,j=2,k;
		System.out.println(i+j);
		System.out.println(i-j);
		System.out.println(i*j);
		System.out.println(i/j);
		System.out.println("% = "+(i%j));
		k = i+j;
		System.out.println("k = "+k);
	}
}
